{
  "author": {
    "name": "Strongpoint Salesforce Integration",
    "email": "support@strongpoint.io",
    "url": "https://strongpoint.io"
  },
  "defaultLocale": "en",
  "private": false,
  "singleInstall": true,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html",      
      "nav_bar": "assets/configuration.html"
    }
  },
  "domainWhitelist": ["server-sf.herokuapp.com", "{{setting.account}}.salesforce.com"],
  "parameters": [  
    {
      "name": "consumeri",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "consumers",
      "type": "text",
      "required": true,
      "secure": true
    },           
    {
      "name": "requestApproveGroups",
      "type": "hidden",
      "default": "null",
      "secure": false
    },
    {
      "name": "approveGroups",
      "type": "hidden",
      "default": "null",
      "secure": false
    },
    {
      "name": "approvalProcess",
      "type": "hidden",
      "required": false,
      "default": "null",
      "secure": false
    }
  ],
  "version": "2.0",
  "frameworkVersion": "2.0"
  
}